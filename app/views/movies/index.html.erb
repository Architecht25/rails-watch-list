<div class="container mt-5">
  <h1 class="text-primary">Bienvenue sur ma Watch List 🎬</h1>
  <p class="lead">Ceci est ma première app Rails.</p>

  <% @movies.each do |movie| %>
    <ul>
      <li><%= link_to movie.title, movie_path(movie) %></li>
    </ul>
  <%end%>

<%= link_to "Ajouter un film", new_movie_path(@movie), class: "btn btn-success" %>
